<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../rng/doc_directive.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<doc_directives>
    <default_binding source="InstanceList">
        <instance ref="outline_sample.xml"/>
    </default_binding>
    <doc_binding source="ddr0 ddr1">
        <merge except="instance_name base_address"/>
        <doc_binding source="registers">
            <merge diff_criteria="register_name field"></merge></doc_binding>
        <doc_binding source="mem_map">
            <split diff_criteria="address_block"/>    
        </doc_binding>
    </doc_binding>
    <doc_binding source="wriop">
        <instance ref="split"/>
    </doc_binding>
</doc_directives>
