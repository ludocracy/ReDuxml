<?xml version="1.0" encoding="UTF-8"?>
<!-- this file is an example of a non-secure user list for designOS to store each user's allowed views of the design  -->
<?xml-model href="file:////CDE-SMB-TX30/b33791/schemas/designos/registry.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<registry xmlns="" id="FSL-DesignOS">
    <!-- the owner of this file is the admin of the system -->
    <owner id="b33791">
        <name>Peter Kong</name>
    </owner>
    <description>List of all users of DesignOS</description>
    <!-- history of changes to users and their views -->
    <history>
        <insert id="asdf" change_ids="b33791" previous="asdf">
            <owner id="b33791">
                <name>Peter Kong</name>
            </owner>
            <description>created this file</description>
            <date>2015-08-20T19:20:30+01:00</date>
        </insert>
    </history>
    <!-- need to prevent <system> here -->
    <users>
        <user id="b33791" keywords="admin">
            <name>Peter Kong</name>
            <user_templates>
                <user_template next="my_template"/>
                <user_template next="some_other_template"/>
            </user_templates>
        </user>
        <user id="customer0001" keywords="customer">
            <name>Joe Customer</name>
        </user>
    </users>
</registry>
